<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="description" content="We search hundreds of deal web sites to find all the recent deals for the product you want to buy. You can search for electronics, apparel, coupons, freebies, local deals and more." />
    <link href="@Url.Content("~/Content/HomePage.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/This/DealsJScript.js")" type="text/javascript"></script>
    @if (false)
    {
        <script src="@Url.Content("~/Scripts/jquery-1.5.1-vsdoc.js")" type="text/javascript"></script>
    }
    @Html.Partial("_GoogleAnalytics")
</head>
<body>
    <div id="header">
        <div class="headerElements">
            @if (IsSectionDefined("Header"))
            {
                @RenderSection("Header")
            }
            else
            {
                @Html.Partial("_PartialHeader")
            }
        </div>
    </div>

    <div id="content">
        @RenderBody()
    </div>

    

        @if (IsSectionDefined("Footer"))
        {
            <div id="footer">
                @RenderSection("Footer")
            </div>
        }

    
        
</body>
</html>
